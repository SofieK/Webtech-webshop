extends layout
extends nav

block content
	h1 ORDER nr= #{orderdetail._id}
	table
		- var i = 0;
		- each order in orders
			tr
				td 
					img(src="/uploads/"+order.Picture)
				td 
					p= orderdetail.order[i].amount
					- i++;
				td 
					h2= order.Name
				td
					p= order.Price
				td 
					p= orderdetail.status